	private Expression getPredefinedMatrix(MatrixType type)  {
		switch (type) {
			case PAULIX :
				return getPauliX();
			case PAULIY :
				return getPauliY();
			case PAULIZ :
				return getPauliZ();
			case HADAMARD :
				return getHadamard();
			case M0:
				return getM0();
			case M1:
				return getM1();
			case CNOT :
				return getCnot();
			case TOFFOLI :
				return getToffoli();
			case FREDKIN :
				return getFredkin();
			case SWAP :
				return getSwap();
			default:
			    assert false;
			    return null;
		}
	}

